{
  "name": "electron-recipes",
  "version": "0.0.1",
  "description": "Recipe management app built in electron",
  "keywords": [
    "electron",
    "react",
    "recipes",
    "redux"
  ],
  "homepage": "https://github.com/ruiquelhas/electron-recipes#readme",
  "bugs": {
    "url": "https://github.com/ruiquelhas/electron-recipes/issues"
  },
  "license": "MIT",
  "author": "Rui Quelhas <rfpquelhas@gmail.com>",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruiquelhas/electron-recipes.git"
  },
  "scripts": {
    "cleanup": "rm -rf dist",
    "compile": "npm run cleanup && babel src --out-dir dist",
    "cover": "npm run compile && istanbul cover _mocha -- $npm_package_options_mocha && npm run cleanup",
    "lint": "standard main.js src/* test/*",
    "poststart": "npm run cleanup",
    "posttest": "npm run cleanup",
    "prepublish": "chmod -R +x ./bin/",
    "seed": "./bin/seed",
    "start": "npm run compile && ./node_modules/.bin/electron main.js",
    "test": "npm run compile && NODE_ENV=test mocha $npm_package_options_mocha"
  },
  "dependencies": {
    "hoek": "^4.0.2",
    "leveldown": "^1.4.6",
    "levelup": "^1.3.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.12.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "deep-freeze": "0.0.1",
    "electron-prebuilt": "1.1.3",
    "enzyme": "^2.4.1",
    "faker": "^3.1.0",
    "istanbul": "^0.4.5",
    "lodash.times": "^4.3.2",
    "mocha": "^3.0.2",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.3.1",
    "spectron": "^3.3.0",
    "standard": "^8.0.0"
  },
  "engine": {
    "node": ">=6"
  },
  "options": {
    "mocha": "--compilers js:babel-register --timeout 10000 --recursive test"
  },
  "pre-commit": [
    "lint"
  ]
}
